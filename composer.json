{
  "name": "mtdowling/jmespath.php",
  "description": "Declaratively specify how to extract elements from a JSON document",
  "keywords": ["json", "jsonpath"],
  "license": "MIT",

  "authors": [
    {
      "name": "Michael Dowling",
      "email": "mtdowling@gmail.com",
      "homepage": "https://github.com/mtdowling"
    }
  ],

  "require": {
    "php": ">=5.3.0"
  },

  "require-dev": {
    "phpunit/phpunit": "~3",
    "boto/jmespath": "*@dev"
  },

  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "boto/jmespath",
        "version": "dev",
        "source": {
          "type": "git",
          "url": "https://github.com/boto/jmespath",
          "reference": "develop"
        }
      }
    }
  ],

  "autoload": {
    "psr-0": {
      "JmesPath": "src/"
    },
    "files": ["src/JmesPath/functions.php"]
  },

  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
